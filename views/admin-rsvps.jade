extends admin

block append controllers
    script(src="/js/controllers/adminRsvps.js")
    script.
        app.value('rsvps', !{JSON.stringify(rsvps)});

block display    
    div(ng-controller="AdminRsvps")
        h3 Reservations (#{rsvps.length})
        each rsvp in rsvps
            .clearfix.row
                .col-md-3 #{rsvp.person.name}
                .col-md-1 &nbsp;
                .col-md-3 #{rsvp.person.email}